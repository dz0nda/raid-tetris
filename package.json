{
    "name": "red-tetris",
    "version": "1.0.0",
    "description": "",
    "keywords": [],
    "author": "",
    "license": "",
    "scripts": {
        "client:dist": "webpack --mode production --progress",
        "client:dev": "webpack-dev-server --mode development --devtool inline-source-map",
        "server:dist": "babel src --out-dir dist",
        "server:dev": "nodemon --exec babel-node ./src/server/server.js",
        "dev": "concurrently --kill-others \"npm run client:dev\" \"npm run server:dev\"",
        "build": "npm run client:dist && npm run server:dist",
        "start": "node dist/server/server.js",
        "lint": "eslint src",
        "lint:fix": "eslint --fix src",
        "format": "prettier './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json",
        "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "coverage": "npm run test",
        "test": "npm run test:all",
        "test:all": "jest test/**/**/*.*.test.js --verbose --coverage --detectOpenHandles --forceExit",
        "test:client": "jest test/client/**/*.test.js --verbose --coverage",
        "test:server": "jest test/server/**/*.test.js --verbose --coverage",
        "test:watch": "npm test -- --watch-All"
    },
    "dependencies": {
        "@material-ui/core": "^4.12.4",
        "@material-ui/icons": "^4.11.3",
        "clsx": "^1.1.1",
        "connected-react-router": "^6.8.0",
        "core-js": "^3.22.4",
        "express": "^4.17.3",
        "history": "^4.10.1",
        "notistack": "^1.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-redux": "^7.2.8",
        "react-router-dom": "^5.2.0",
        "react-virtualized": "^9.22.3",
        "redux": "^4.2.0",
        "redux-thunk": "^2.4.1",
        "regenerator-runtime": "^0.13.9",
        "socket.io": "^3.1.2",
        "socket.io-client": "^3.1.3",
        "uuid": "^8.3.2",
        "winston": "^2.4.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/node": "^7.17.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "babel-loader": "^8.2.5",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^7.1.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "enzyme-to-json": "^3.6.1",
        "eslint": "^8.15.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.16",
        "prettier": "^2.2.1",
        "randomstring": "^1.2.2",
        "testdouble": "^3.16.5",
        "url-loader": "^4.1.1",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js"
        }
    }
}
